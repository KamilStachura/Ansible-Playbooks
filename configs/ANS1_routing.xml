  <config>
    <native xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-native">
      <router operation="replace">
                <bgp xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-bgp">
          <id>65001</id>
          <bgp>
            <log-neighbor-changes>true</log-neighbor-changes>
            <router-id>
              <ip-id>1.1.1.1</ip-id>
            </router-id>
          </bgp>
                    <neighbor>
            <id>1.1.1.2</id>
            <remote-as>65001</remote-as>
            <update-source>
              <Loopback>0</Loopback>
            </update-source>
          </neighbor>
                    <address-family>
            <no-vrf>
              <ipv4>
                <af-name>unicast</af-name>
                <ipv4-unicast>
                                    <neighbor>
                    <id>1.1.1.2</id>
                    <activate/>
                  </neighbor>
                                                      <network>
                    <with-mask>
                      <number>10.10.10.0</number>
                      <mask>255.255.255.0</mask>
                    </with-mask>
                  </network>
                                    <network>
                    <with-mask>
                      <number>11.11.11.0</number>
                      <mask>255.255.255.0</mask>
                    </with-mask>
                  </network>
                                  </ipv4-unicast>
              </ipv4>
            </no-vrf>
          </address-family>
        </bgp>
                        <ospf xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-ospf">
          <id>1</id>
          <router-id>1.1.1.1</router-id>
                    <network>
            <ip>1.1.1.1</ip>
            <mask>0.0.0.0</mask>
            <area>0</area>
          </network>
                    <network>
            <ip>172.168.100.0</ip>
            <mask>0.0.0.255</mask>
            <area>0</area>
          </network>
                  </ospf>
                      </router>
    </native>
  </config>
